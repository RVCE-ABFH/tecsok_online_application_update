            function PrintPanel() {
    
                var panel = document.getElementById('divToPrint');
                var printWindow = window.open('', '', 'height=auto,width=auto');
                printWindow.document.write('<html><head><title>Profile Details</title>   <link href="../stylesheets/Site1.css" id="Link1" rel="stylesheet" type="text/css" /> ');
