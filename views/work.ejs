    <script type="text/javascript" language="javascript">

        function ConfirmNext() {

            if ($('[id="FormsPH_rdNo"]').is(':checked')) {
                location.href = "PhotoSignForm.aspx";
                return false;
            }
            else {
                if (confirm("Details has been saved successfully.Do you want to proceed ?")) {
                    location.href = "PhotoSignForm.aspx";
                }
                else {
                    return false;
                }
                return false;
            }
        }

        function RedirectPerDetail(LangID) {
            if (LangID == 1) {
                alert('Please Submit Personal Details.');
            }
            else {
                alert('Please Submit Personal Details.');
            }
            location.href = "../../Profile/PersonalForm.aspx";
        }

        function RequiredField(LangID) {
            if (LangID == 1) {
                alert('* mark fields are mandatory.');
            }
            else {
                alert('* mark fields are mandatory.');
            }
        }

        function CheckEditExp() {
            var EditExp = $("#FormsPH_hdv_valueExp").val();

            if (EditExp != '0') {
                alert($("#FormsPH_hdnEditRow").val());
                return false;
            }
            else {
                var msg = $("#FormsPH_hdnConfirmExp").val();
                return confirm(msg);
            }
            return true;
        }


        function ConfirmExperience(objrb) {
            debugger;
            switch (objrb.id) {
                case "FormsPH_rdYes":

                    if (objrb.checked) {
                        __doPostBack();
                    }
                    break;
                case "FormsPH_rdNo":

                    if (objrb.checked) {
                        var RowCount = parseInt(document.getElementById('FormsPH_hdnRowCount').value);

                        if (RowCount == 0) {
                            __doPostBack();
                        }
                        else if (RowCount > 0) {
                            if (confirm("Filled experience(s) will be deleted automatically. are you want to continue. ")) {
                                __doPostBack();
                            }
                            else {
                                $('#FormsPH_rdYes').prop('checked', true);
                                return false;
                            }
                        }
                    }

                    break;
            }

        }
    </script>
